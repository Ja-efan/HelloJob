{
    "mcpServers": {
        "dart-mcp": {
            "command": "python", 
            "args": ["/root/hellojob/app/mcp/dart_mcp/dart.py"],
            "env": {
                "DART_API_KEY": "YOUR_DART_API_KEY_IN_DOTENV"
            }
        },
        "naver-search": {
            "command": "/usr/bin/node",
            "args": ["/root/hellojob/app/mcp/naver-search-mcp/dist/src/index.js"],
            "env": {
                "NAVER_CLIENT_ID": "YOUR_NAVER_CLIENT_ID_IN_DOTENV",
                "NAVER_CLIENT_SECRET": "YOUR_NAVER_CLIENT_SECRET_IN_DOTENV"
            }
        },
        "sequential-thinking": {
            "command": "/usr/bin/npx",
            "args": [
                "-y",
                "@smithery/cli@latest",
                "run",
                "@smithery-ai/server-sequential-thinking",
                "--key",
                "a6c0e55b-6ef1-44f7-83af-aac975f23805"
            ]
        }
    }
}
